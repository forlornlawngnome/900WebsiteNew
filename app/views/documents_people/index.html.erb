<p id="notice"><%= notice %></p>

<h1>Listing Documents People</h1>

<table>
  <thead>
    <tr>
      <th>Person</th>
      <th>Document</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @documents_people.each do |documents_person| %>
      <tr>
        <td><%= documents_person.person %></td>
        <td><%= documents_person.document %></td>
        <td><%= link_to 'Show', documents_person %></td>
        <td><%= link_to 'Edit', edit_documents_person_path(documents_person) %></td>
        <td><%= link_to 'Destroy', documents_person, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Documents person', new_documents_person_path %>
