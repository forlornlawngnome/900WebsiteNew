<p id="notice"><%= notice %></p>

<h1>Listing About Photos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @about_photos.each do |about_photo| %>
      <tr>
        <td><%= about_photo.name %></td>
        <td><%= about_photo.image %></td>
        <td><%= link_to 'Show', about_photo %></td>
        <td><%= link_to 'Edit', edit_about_photo_path(about_photo) %></td>
        <td><%= link_to 'Destroy', about_photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New About photo', new_about_photo_path %>
