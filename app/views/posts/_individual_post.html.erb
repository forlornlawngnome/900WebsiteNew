<h5><%=post.title%> <small><i><%=post.date_published%></i></small></h5>
<%post.categories.each do |cat|%>
	<%=fa_icon "tag"%> <%=category_link(cat)%>
<%end%>
<hr />
<%post.post_contents.order("post_order").each do |content|%>
	<p>
		<%=render "/post_contents/post_content", :content=>content%>
	</p>
<%end%>