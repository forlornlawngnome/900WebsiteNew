<p id="notice"><%= notice %></p>

<h1>Listing About Contents</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @about_contents.each do |about_content| %>
      <tr>
        <td><%= about_content.name %></td>
        <td><%= about_content.title %></td>
        <td><%= about_content.content %></td>
        <td><%= link_to 'Show', about_content %></td>
        <td><%= link_to 'Edit', edit_about_content_path(about_content) %></td>
        <td><%= link_to 'Destroy', about_content, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New About content', new_about_content_path %>
